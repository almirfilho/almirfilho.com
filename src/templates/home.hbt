{{> head }}

<section class="alm-main-content">
    <ul class="alm-post-list">
        {{# each collections.posts }}
        <li class="alm-post-list--item">
            <a class="alm-post-list--item-link" href="{{ this.path }}/">
                <span class="alm-post--title">{{ this.title }}</span>
            </a>
        </li>
        {{/ each }}
    </ul>
</section>

{{> tail }}